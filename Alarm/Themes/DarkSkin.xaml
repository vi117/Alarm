<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    >
    <SolidColorBrush x:Key="InnerBackgroundColor" Color="#1e1e1e"/>
    <SolidColorBrush x:Key="BackgroundColor" Color="#2d2d30"/>
    <SolidColorBrush x:Key="TitleBarColor" Color="#2d2d30"/>
    <SolidColorBrush x:Key="CharacterColor" Color="White"/>
    <SolidColorBrush x:Key="ButtonBackgroundColor" Color="#2d2d30"/>
    <SolidColorBrush x:Key="GridSplitterColor" Color="#2d2d30"/>
    
    <SolidColorBrush x:Key="ComboBox.BackgroundColor" Color="#3F3F46"/>
    <SolidColorBrush x:Key="ComboBox.ForegroundColor" Color="White"/>
    <SolidColorBrush x:Key="ComboBox.BorderColor" Color="#434346"/>
    <SolidColorBrush x:Key="ComboBox.MouseOverFocusColor" Color="#007ACC"/>
    
    <SolidColorBrush x:Key="ComboBoxItem.DefaultColor" Color="#1B1C1D"/>
    <SolidColorBrush x:Key="ComboBoxItem.DefaultCharacterColor" Color="White"/>
    <SolidColorBrush x:Key="ComboBoxItem.SelectedColor" Color="#3F3F46"/>
    <SolidColorBrush x:Key="ComboBoxItem.SelectedCharacterColor" Color="White"/>
    <SolidColorBrush x:Key="ComboBoxItem.HighlightColor" Color="#3F3F46"/>
    
    <SolidColorBrush x:Key="InputBackgroundColor" Color="#3F3F46"/>
</ResourceDictionary>