<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    >
    <SolidColorBrush x:Key="InnerBackgroundColor" Color="#f8f9fa"/>
    <SolidColorBrush x:Key="BackgroundColor" Color="#f8f9fa"/>
    <SolidColorBrush x:Key="TitleBarColor" Color="#41b1ff"/>
    <SolidColorBrush x:Key="CharacterColor" Color="Black"/>
    <SolidColorBrush x:Key="ButtonBackgroundColor" Color="#d5d5d5"/>
    <SolidColorBrush x:Key="GridSplitterColor" Color="#d5d5d5"/>
    
    <SolidColorBrush x:Key="ComboBox.BackgroundColor" Color="White"/>
    <SolidColorBrush x:Key="ComboBox.ForegroundColor" Color="Black"/>
    <SolidColorBrush x:Key="ComboBox.BorderColor" Color="#CECECE"/>
    <SolidColorBrush x:Key="ComboBox.MouseOverFocusColor" Color="#007ACC"/>
    
    <SolidColorBrush x:Key="ComboBoxItem.DefaultColor" Color="White"/>
    <SolidColorBrush x:Key="ComboBoxItem.DefaultCharacterColor" Color="Black"/>
    <SolidColorBrush x:Key="ComboBoxItem.SelectedColor" Color="#41b1e1"/>
    <SolidColorBrush x:Key="ComboBoxItem.SelectedCharacterColor" Color="White"/>
    <SolidColorBrush x:Key="ComboBoxItem.HighlightColor" Color="#A0D8F0"/>
    
    <SolidColorBrush x:Key="InputBackgroundColor" Color="White"/>
</ResourceDictionary>